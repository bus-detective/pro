<div class="row">
  <div class="col-md-3">
    <div class="card">
      <div class="card-header">Filters</div>
      <div class="card-block">
        {{#each model.vehicles as |vehicle|}}
          <label>
            {{input type="checkbox" checked=vehicle.isSelected}}
            {{vehicle.remoteId}}
          </label>
        {{/each}}
      </div>
    </div>
  </div>
  <div class="col-md-9">
    {{outlet}} 
  </div>
</div>


